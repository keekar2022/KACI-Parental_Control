parental_control_state.json

# Log files (contain sensitive usage data)
/var/log/parental_control*.jsonl
*.jsonl

# Configuration files (may contain sensitive settings)
config/app/config.json
backend/config.json

# Temporary files
*.tmp
*.swp
*.swo
*~

# IDE and editor files
.vscode/
.idea/
*.code-workspace

# OS files
.DS_Store
Thumbs.db
desktop.ini

# Keep example files
!*.example
!config.example/
!config.example/**

# Build artifacts
build/
dist/

# Credentials
credentials.txt
*.pem
*.key
*.crt

# pfSense specific
/cf/conf/config.xml.backup*
